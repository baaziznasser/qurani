{
	"number": 37,
	"name": "سُورَةُ الصَّافَّاتِ",
	"englishName": "As-Saaffaat",
	"englishNameTranslation": "Those drawn up in Ranks",
	"revelationType": "Meccan",
	"numberOfAyahs": 182,
	"ayahs": [
		{
			"number": 3789,
			"text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَالصَّافَّاتِ صَفًّا",
			"numberInSurah": 1,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3790,
			"text": "فَالزَّاجِرَاتِ زَجْرًا",
			"numberInSurah": 2,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3791,
			"text": "فَالتَّالِيَاتِ ذِكْرًا",
			"numberInSurah": 3,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3792,
			"text": "إِنَّ إِلَٰهَكُمْ لَوَاحِدٌ",
			"numberInSurah": 4,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3793,
			"text": "رَبُّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا وَرَبُّ الْمَشَارِقِ",
			"numberInSurah": 5,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3794,
			"text": "إِنَّا زَيَّنَّا السَّمَاءَ الدُّنْيَا بِزِينَةٍ الْكَوَاكِبِ",
			"numberInSurah": 6,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3795,
			"text": "وَحِفْظًا مِنْ كُلِّ شَيْطَانٍ مَارِدٍ",
			"numberInSurah": 7,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3796,
			"text": "لَا يَسَّمَّعُونَ إِلَى الْمَلَإِ الْأَعْلَىٰ وَيُقْذَفُونَ مِنْ كُلِّ جَانِبٍ",
			"numberInSurah": 8,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3797,
			"text": "دُحُورًا ۖ وَلَهُمْ عَذَابٌ وَاصِبٌ",
			"numberInSurah": 9,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3798,
			"text": "إِلَّا مَنْ خَطِفَ الْخَطْفَةَ فَأَتْبَعَهُ شِهَابٌ ثَاقِبٌ",
			"numberInSurah": 10,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3799,
			"text": "فَاسْتَفْتِهِمْ أَهُمْ أَشَدُّ خَلْقًا أَمْ مَنْ خَلَقْنَا ۚ إِنَّا خَلَقْنَاهُمْ مِنْ طِينٍ لَازِبٍ",
			"numberInSurah": 11,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3800,
			"text": "بَلْ عَجِبْتَ وَيَسْخَرُونَ",
			"numberInSurah": 12,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3801,
			"text": "وَإِذَا ذُكِّرُوا لَا يَذْكُرُونَ",
			"numberInSurah": 13,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3802,
			"text": "وَإِذَا رَأَوْا آيَةً يَسْتَسْخِرُونَ",
			"numberInSurah": 14,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3803,
			"text": "وَقَالُوا إِنْ هَٰذَا إِلَّا سِحْرٌ مُبِينٌ",
			"numberInSurah": 15,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3804,
			"text": "أَإِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَامًا أَإِنَّا لَمَبْعُوثُونَ",
			"numberInSurah": 16,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3805,
			"text": "أَوَآبَاؤُنَا الْأَوَّلُونَ",
			"numberInSurah": 17,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3806,
			"text": "قُلْ نَعَمْ وَأَنْتُمْ دَاخِرُونَ",
			"numberInSurah": 18,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3807,
			"text": "فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ فَإِذَا هُمْ يَنْظُرُونَ",
			"numberInSurah": 19,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3808,
			"text": "وَقَالُوا يَا وَيْلَنَا هَٰذَا يَوْمُ الدِّينِ",
			"numberInSurah": 20,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3809,
			"text": "هَٰذَا يَوْمُ الْفَصْلِ الَّذِي كُنْتُمْ بِهِ تُكَذِّبُونَ",
			"numberInSurah": 21,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 386,
			"hizbQuarter": 178,
			"sajda": false
		},
		{
			"number": 3810,
			"text": "۞ احْشُرُوا الَّذِينَ ظَلَمُوا وَأَزْوَاجَهُمْ وَمَا كَانُوا يَعْبُدُونَ",
			"numberInSurah": 22,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3811,
			"text": "مِنْ دُونِ اللَّهِ فَاهْدُوهُمْ إِلَىٰ صِرَاطِ الْجَحِيمِ",
			"numberInSurah": 23,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3812,
			"text": "وَقِفُوهُمْ ۖ إِنَّهُمْ مَسْئُولُونَ",
			"numberInSurah": 24,
			"juz": 23,
			"manzil": 6,
			"page": 446,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3813,
			"text": "مَا لَكُمْ لَا تَنَاصَرُونَ",
			"numberInSurah": 25,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3814,
			"text": "بَلْ هُمُ الْيَوْمَ مُسْتَسْلِمُونَ",
			"numberInSurah": 26,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3815,
			"text": "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَاءَلُونَ",
			"numberInSurah": 27,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3816,
			"text": "قَالُوا إِنَّكُمْ كُنْتُمْ تَأْتُونَنَا عَنِ الْيَمِينِ",
			"numberInSurah": 28,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3817,
			"text": "قَالُوا بَلْ لَمْ تَكُونُوا مُؤْمِنِينَ",
			"numberInSurah": 29,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3818,
			"text": "وَمَا كَانَ لَنَا عَلَيْكُمْ مِنْ سُلْطَانٍ ۖ بَلْ كُنْتُمْ قَوْمًا طَاغِينَ",
			"numberInSurah": 30,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3819,
			"text": "فَحَقَّ عَلَيْنَا قَوْلُ رَبِّنَا ۖ إِنَّا لَذَائِقُونَ",
			"numberInSurah": 31,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3820,
			"text": "فَأَغْوَيْنَاكُمْ إِنَّا كُنَّا غَاوِينَ",
			"numberInSurah": 32,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3821,
			"text": "فَإِنَّهُمْ يَوْمَئِذٍ فِي الْعَذَابِ مُشْتَرِكُونَ",
			"numberInSurah": 33,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3822,
			"text": "إِنَّا كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَ",
			"numberInSurah": 34,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3823,
			"text": "إِنَّهُمْ كَانُوا إِذَا قِيلَ لَهُمْ لَا إِلَٰهَ إِلَّا اللَّهُ يَسْتَكْبِرُونَ",
			"numberInSurah": 35,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3824,
			"text": "وَيَقُولُونَ أَئِنَّا لَتَارِكُو آلِهَتِنَا لِشَاعِرٍ مَجْنُونٍ",
			"numberInSurah": 36,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3825,
			"text": "بَلْ جَاءَ بِالْحَقِّ وَصَدَّقَ الْمُرْسَلِينَ",
			"numberInSurah": 37,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3826,
			"text": "إِنَّكُمْ لَذَائِقُو الْعَذَابِ الْأَلِيمِ",
			"numberInSurah": 38,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3827,
			"text": "وَمَا تُجْزَوْنَ إِلَّا مَا كُنْتُمْ تَعْمَلُونَ",
			"numberInSurah": 39,
			"juz": 23,
