			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3828,
			"text": "إِلَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ",
			"numberInSurah": 40,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3829,
			"text": "أُولَٰئِكَ لَهُمْ رِزْقٌ مَعْلُومٌ",
			"numberInSurah": 41,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3830,
			"text": "فَوَاكِهُ ۖ وَهُمْ مُكْرَمُونَ",
			"numberInSurah": 42,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3831,
			"text": "فِي جَنَّاتِ النَّعِيمِ",
			"numberInSurah": 43,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3832,
			"text": "عَلَىٰ سُرُرٍ مُتَقَابِلِينَ",
			"numberInSurah": 44,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3833,
			"text": "يُطَافُ عَلَيْهِمْ بِكَأْسٍ مِنْ مَعِينٍ",
			"numberInSurah": 45,
			"juz": 23,
			"manzil": 6,
			"page": 447,
			"ruku": 387,
			"hizbQuarter": 179,
			"sajda": false
		},
		{
			"number": 3834,
			"text": "بَيْضَاءَ لَذَّةٍ لِلشَّارِبِينَ",
