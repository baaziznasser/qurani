{
	"number": 50,
	"name": "سُورَةُ قٓ",
	"englishName": "Qaaf",
	"englishNameTranslation": "The letter Qaaf",
	"revelationType": "Meccan",
	"numberOfAyahs": 45,
	"ayahs": [
		{
			"number": 4631,
			"text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ ق ۚ وَالْقُرْآنِ الْمَجِيدِ",
			"numberInSurah": 1,
			"juz": 26,
			"manzil": 7,
			"page": 518,
			"ruku": 452,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4632,
			"text": "بَلْ عَجِبُوا أَنْ جَاءَهُمْ مُنْذِرٌ مِنْهُمْ فَقَالَ الْكَافِرُونَ هَٰذَا شَيْءٌ عَجِيبٌ",
			"numberInSurah": 2,
			"juz": 26,
			"manzil": 7,
			"page": 518,
			"ruku": 452,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4633,
			"text": "أَإِذَا مِتْنَا وَكُنَّا تُرَابًا ۖ ذَٰلِكَ رَجْعٌ بَعِيدٌ",
			"numberInSurah": 3,
			"juz": 26,
			"manzil": 7,
			"page": 518,
			"ruku": 452,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4634,
			"text": "قَدْ عَلِمْنَا مَا تَنْقُصُ الْأَرْضُ مِنْهُمْ ۖ وَعِنْدَنَا كِتَابٌ حَفِيظٌ",
			"numberInSurah": 4,
			"juz": 26,
			"manzil": 7,
			"page": 518,
			"ruku": 452,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4635,
			"text": "بَلْ كَذَّبُوا بِالْحَقِّ لَمَّا جَاءَهُمْ فَهُمْ فِي أَمْرٍ مَرِيجٍ",
			"numberInSurah": 5,
			"juz": 26,
			"manzil": 7,
			"page": 518,
			"ruku": 452,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4636,
			"text": "أَفَلَمْ يَنْظُرُوا إِلَى السَّمَاءِ فَوْقَهُمْ كَيْفَ بَنَيْنَاهَا وَزَيَّنَّاهَا وَمَا لَهَا مِنْ فُرُوجٍ",
			"numberInSurah": 6,
			"juz": 26,
			"manzil": 7,
			"page": 518,
			"ruku": 452,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4637,
			"text": "وَالْأَرْضَ مَدَدْنَاهَا وَأَلْقَيْنَا فِيهَا رَوَاسِيَ وَأَنْبَتْنَا فِيهَا مِنْ كُلِّ زَوْجٍ بَهِيجٍ",
			"numberInSurah": 7,
			"juz": 26,
			"manzil": 7,
			"page": 518,
			"ruku": 452,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4638,
			"text": "تَبْصِرَةً وَذِكْرَىٰ لِكُلِّ عَبْدٍ مُنِيبٍ",
			"numberInSurah": 8,
			"juz": 26,
			"manzil": 7,
			"page": 518,
			"ruku": 452,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4639,
			"text": "وَنَزَّلْنَا مِنَ السَّمَاءِ مَاءً مُبَارَكًا فَأَنْبَتْنَا بِهِ جَنَّاتٍ وَحَبَّ الْحَصِيدِ",
			"numberInSurah": 9,
			"juz": 26,
			"manzil": 7,
			"page": 518,
			"ruku": 452,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4640,
			"text": "وَالنَّخْلَ بَاسِقَاتٍ لَهَا طَلْعٌ نَضِيدٌ",
			"numberInSurah": 10,
			"juz": 26,
			"manzil": 7,
			"page": 518,
			"ruku": 452,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4641,
			"text": "رِزْقًا لِلْعِبَادِ ۖ وَأَحْيَيْنَا بِهِ بَلْدَةً مَيْتًا ۚ كَذَٰلِكَ الْخُرُوجُ",
			"numberInSurah": 11,
			"juz": 26,
			"manzil": 7,
			"page": 518,
			"ruku": 452,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4642,
			"text": "كَذَّبَتْ قَبْلَهُمْ قَوْمُ نُوحٍ وَأَصْحَابُ الرَّسِّ وَثَمُودُ",
			"numberInSurah": 12,
			"juz": 26,
			"manzil": 7,
			"page": 518,
			"ruku": 452,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4643,
			"text": "وَعَادٌ وَفِرْعَوْنُ وَإِخْوَانُ لُوطٍ",
			"numberInSurah": 13,
			"juz": 26,
			"manzil": 7,
			"page": 518,
			"ruku": 452,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4644,
			"text": "وَأَصْحَابُ الْأَيْكَةِ وَقَوْمُ تُبَّعٍ ۚ كُلٌّ كَذَّبَ الرُّسُلَ فَحَقَّ وَعِيدِ",
			"numberInSurah": 14,
			"juz": 26,
			"manzil": 7,
			"page": 518,
			"ruku": 452,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4645,
			"text": "أَفَعَيِينَا بِالْخَلْقِ الْأَوَّلِ ۚ بَلْ هُمْ فِي لَبْسٍ مِنْ خَلْقٍ جَدِيدٍ",
			"numberInSurah": 15,
			"juz": 26,
			"manzil": 7,
			"page": 518,
			"ruku": 452,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4646,
			"text": "وَلَقَدْ خَلَقْنَا الْإِنْسَانَ وَنَعْلَمُ مَا تُوَسْوِسُ بِهِ نَفْسُهُ ۖ وَنَحْنُ أَقْرَبُ إِلَيْهِ مِنْ حَبْلِ الْوَرِيدِ",
			"numberInSurah": 16,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 453,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4647,
			"text": "إِذْ يَتَلَقَّى الْمُتَلَقِّيَانِ عَنِ الْيَمِينِ وَعَنِ الشِّمَالِ قَعِيدٌ",
			"numberInSurah": 17,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 453,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4648,
			"text": "مَا يَلْفِظُ مِنْ قَوْلٍ إِلَّا لَدَيْهِ رَقِيبٌ عَتِيدٌ",
			"numberInSurah": 18,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 453,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4649,
			"text": "وَجَاءَتْ سَكْرَةُ الْمَوْتِ بِالْحَقِّ ۖ ذَٰلِكَ مَا كُنْتَ مِنْهُ تَحِيدُ",
			"numberInSurah": 19,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 453,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4650,
			"text": "وَنُفِخَ فِي الصُّورِ ۚ ذَٰلِكَ يَوْمُ الْوَعِيدِ",
			"numberInSurah": 20,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 453,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4651,
			"text": "وَجَاءَتْ كُلُّ نَفْسٍ مَعَهَا سَائِقٌ وَشَهِيدٌ",
			"numberInSurah": 21,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 453,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4652,
			"text": "لَقَدْ كُنْتَ فِي غَفْلَةٍ مِنْ هَٰذَا فَكَشَفْنَا عَنْكَ غِطَاءَكَ فَبَصَرُكَ الْيَوْمَ حَدِيدٌ",
			"numberInSurah": 22,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 453,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4653,
			"text": "وَقَالَ قَرِينُهُ هَٰذَا مَا لَدَيَّ عَتِيدٌ",
			"numberInSurah": 23,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 453,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4654,
			"text": "أَلْقِيَا فِي جَهَنَّمَ كُلَّ كَفَّارٍ عَنِيدٍ",
			"numberInSurah": 24,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 453,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4655,
			"text": "مَنَّاعٍ لِلْخَيْرِ مُعْتَدٍ مُرِيبٍ",
			"numberInSurah": 25,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 453,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4656,
			"text": "الَّذِي جَعَلَ مَعَ اللَّهِ إِلَٰهًا آخَرَ فَأَلْقِيَاهُ فِي الْعَذَابِ الشَّدِيدِ",
			"numberInSurah": 26,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 453,
			"hizbQuarter": 207,
			"sajda": false
		},
		{
			"number": 4657,
			"text": "۞ قَالَ قَرِينُهُ رَبَّنَا مَا أَطْغَيْتُهُ وَلَٰكِنْ كَانَ فِي ضَلَالٍ بَعِيدٍ",
			"numberInSurah": 27,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 453,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
			"number": 4658,
			"text": "قَالَ لَا تَخْتَصِمُوا لَدَيَّ وَقَدْ قَدَّمْتُ إِلَيْكُمْ بِالْوَعِيدِ",
			"numberInSurah": 28,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 453,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
			"number": 4659,
			"text": "مَا يُبَدَّلُ الْقَوْلُ لَدَيَّ وَمَا أَنَا بِظَلَّامٍ لِلْعَبِيدِ",
			"numberInSurah": 29,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 453,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
			"number": 4660,
			"text": "يَوْمَ نَقُولُ لِجَهَنَّمَ هَلِ امْتَلَأْتِ وَتَقُولُ هَلْ مِنْ مَزِيدٍ",
			"numberInSurah": 30,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 454,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
			"number": 4661,
			"text": "وَأُزْلِفَتِ الْجَنَّةُ لِلْمُتَّقِينَ غَيْرَ بَعِيدٍ",
			"numberInSurah": 31,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 454,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
			"number": 4662,
			"text": "هَٰذَا مَا تُوعَدُونَ لِكُلِّ أَوَّابٍ حَفِيظٍ",
			"numberInSurah": 32,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 454,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
			"number": 4663,
			"text": "مَنْ خَشِيَ الرَّحْمَٰنَ بِالْغَيْبِ وَجَاءَ بِقَلْبٍ مُنِيبٍ",
			"numberInSurah": 33,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 454,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
			"number": 4664,
			"text": "ادْخُلُوهَا بِسَلَامٍ ۖ ذَٰلِكَ يَوْمُ الْخُلُودِ",
			"numberInSurah": 34,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 454,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
			"number": 4665,
			"text": "لَهُمْ مَا يَشَاءُونَ فِيهَا وَلَدَيْنَا مَزِيدٌ",
			"numberInSurah": 35,
			"juz": 26,
			"manzil": 7,
			"page": 519,
			"ruku": 454,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
			"number": 4666,
			"text": "وَكَمْ أَهْلَكْنَا قَبْلَهُمْ مِنْ قَرْنٍ هُمْ أَشَدُّ مِنْهُمْ بَطْشًا فَنَقَّبُوا فِي الْبِلَادِ هَلْ مِنْ مَحِيصٍ",
			"numberInSurah": 36,
			"juz": 26,
			"manzil": 7,
			"page": 520,
			"ruku": 454,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
			"number": 4667,
			"text": "إِنَّ فِي ذَٰلِكَ لَذِكْرَىٰ لِمَنْ كَانَ لَهُ قَلْبٌ أَوْ أَلْقَى السَّمْعَ وَهُوَ شَهِيدٌ",
			"numberInSurah": 37,
			"juz": 26,
			"manzil": 7,
			"page": 520,
			"ruku": 454,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
			"number": 4668,
			"text": "وَلَقَدْ خَلَقْنَا السَّمَاوَاتِ وَالْأَرْضَ وَمَا بَيْنَهُمَا فِي سِتَّةِ أَيَّامٍ وَمَا مَسَّنَا مِنْ لُغُوبٍ",
			"numberInSurah": 38,
			"juz": 26,
			"manzil": 7,
			"page": 520,
			"ruku": 454,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
			"number": 4669,
			"text": "فَاصْبِرْ عَلَىٰ مَا يَقُولُونَ وَسَبِّحْ بِحَمْدِ رَبِّكَ قَبْلَ طُلُوعِ الشَّمْسِ وَقَبْلَ الْغُرُوبِ",
			"numberInSurah": 39,
			"juz": 26,
			"manzil": 7,
			"page": 520,
			"ruku": 454,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
			"number": 4670,
			"text": "وَمِنَ اللَّيْلِ فَسَبِّحْهُ وَأَدْبَارَ السُّجُودِ",
			"numberInSurah": 40,
			"juz": 26,
			"manzil": 7,
			"page": 520,
			"ruku": 454,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
			"number": 4671,
			"text": "وَاسْتَمِعْ يَوْمَ يُنَادِ الْمُنَادِ مِنْ مَكَانٍ قَرِيبٍ",
			"numberInSurah": 41,
			"juz": 26,
			"manzil": 7,
			"page": 520,
			"ruku": 454,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
			"number": 4672,
			"text": "يَوْمَ يَسْمَعُونَ الصَّيْحَةَ بِالْحَقِّ ۚ ذَٰلِكَ يَوْمُ الْخُرُوجِ",
			"numberInSurah": 42,
			"juz": 26,
			"manzil": 7,
			"page": 520,
			"ruku": 454,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
			"number": 4673,
			"text": "إِنَّا نَحْنُ نُحْيِي وَنُمِيتُ وَإِلَيْنَا الْمَصِيرُ",
			"numberInSurah": 43,
			"juz": 26,
			"manzil": 7,
			"page": 520,
			"ruku": 454,
			"hizbQuarter": 208,
			"sajda": false
		},
		{
