			"juz": 30,
			"manzil": 7,
			"page": 583,
			"ruku": 520,
			"hizbQuarter": 233,
			"sajda": false
		},
		{
			"number": 5720,
			"text": "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ",
